{
  "presets": [
    "es2015"
  ],
  "plugins": [
    "transform-runtime",
    [
      "transform-imports",
      {
        "vuetify": {
          "transform": "vuetify/es5/components/${member}",
          "preventFullImport": true
        }
      }
    ]
  ],
  "env": {
    "production": {
      "presets": [
        [
          "minify",
          {
            "mangle": false
          }
        ]
      ]
    },
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
